{{#each this}}

<div id='{{@ index}}' class='col-offset-md-1 col-md-3'>
    <img class='poster' src='{{Poster}}'>
    <p class='title'>{{Title}}</p>
    <p class='year'>{{Year}}</p>
    <p class='plot'>{{Plot}}</p>
    <p class="userRated">{{userRating}}</p>
    <button class='delete-btn' id='delete{{@ index}}'>Delete</button>
</div>


{{/each}}